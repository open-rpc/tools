{
  "name": "@open-rpc/monaco-editor-react",
  "version": "0.0.0-development",
  "description": "",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:package": "vite build && tsc --emitDeclarationOnly",
    "clean:package": "rm -rf dist && rm -rf tsconfig.tsbuildinfo",
    "build:packagew": "webpack --mode production && tsc --emitDeclarationOnly",
    "start": "vite",
    "startw": "webpack serve --mode development",
    "dev": "webpack serve --mode development",
    "clean:packagew": "rm -rf dist && rm -rf tsconfig.tsbuildinfo",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "test": "npm run lint && jest --coverage --passWithNoTests",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,md}'"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@mui/material": "6.3.1",
    "monaco-editor": "0.52.2"
  },
  "peerDependencies": {
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@babel/core": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.10.0",
    "style-loader": "^3.3.4",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "html-webpack-plugin": "^5.6.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0",
    "webpack": "5.98.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2",
    "vite": "6.0.5"
  },
  "files": ["dist"],
  "engines": {
    "node": ">=18.20.5"
  }
}
