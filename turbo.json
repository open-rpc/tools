{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build:package": {
      "outputs": ["dist/**", "build/**"]
    },
    "build": {
      "dependsOn": ["^build:package"],
      "outputs": ["dist/**", "build/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "start": {
      "dependsOn": ["build"],
      "outputs": []
    }
  }
}
